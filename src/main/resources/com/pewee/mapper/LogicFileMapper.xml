    <delete id="deleteLogicFileById" parameterType="Long">
        delete from tbl_logic_file where id = #{id}
    </delete>

    <delete id="deleteLogicFileByIds" parameterType="String">
        delete from tbl_logic_file where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <update id="logicallyDeleteLogicFileByCodes" parameterType="java.util.List">
        update tbl_logic_file set status = 3 where code in
        <foreach collection="codes" item = "code" open="(" separator="," close=")">
            #{code}
        </foreach>
    </update>

    <select id="selectLogicFileListByCodes" parameterType="java.util.List" resultMap="LogicFileResult">
        <include refid="selectLogicFileVo"/>
        <where>
            code in
            <foreach collection="codes" item = "code" open="(" separator="," close=")">
                #{code}
            </foreach>
        </where>
    </select>
    
