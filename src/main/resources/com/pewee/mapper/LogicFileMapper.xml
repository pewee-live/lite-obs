<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pewee.mapper.LogicFileMapper">
    
    <resultMap type="com.pewee.bean.LogicFile" id="LogicFileResult">
        <result property="id"    column="id"    />
        <result property="code"    column="code"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="fileName"    column="file_name"    />
        <result property="mimeType"    column="mime_type"    />
        <result property="length"    column="length"    />
        <result property="split"    column="split"    />
        <result property="total"    column="total"    />
        <result property="crc32"    column="crc32"    />
        <result property="sysCode"    column="sys_code"    />
        <result property="sysAlisa"    column="sys_alisa"    />
        <result property="status"    column="status"    />
        <result property="expire"    column="expire"    />
    </resultMap>

    <sql id="selectLogicFileVo">
        select id, code, create_time, update_time, file_name, mime_type, length, split, total, crc32, sys_code, sys_alisa, status, expire from tbl_logic_file
    </sql>

    <select id="selectLogicFileList" parameterType="com.pewee.bean.LogicFile" resultMap="LogicFileResult">
        <include refid="selectLogicFileVo"/>
        <where>  
            <if test="code != null  and code != ''"> and code = #{code}</if>
            <if test="fileName != null  and fileName != ''"> and file_name like concat('%', #{fileName}, '%')</if>
            <if test="mimeType != null  and mimeType != ''"> and mime_type = #{mimeType}</if>
            <if test="length != null "> and length = #{length}</if>
            <if test="split != null "> and split = #{split}</if>
            <if test="total != null "> and total = #{total}</if>
            <if test="crc32 != null  and crc32 != ''"> and crc32 = #{crc32}</if>
            <if test="sysCode != null  and sysCode != ''"> and sys_code = #{sysCode}</if>
            <if test="sysAlisa != null  and sysAlisa != ''"> and sys_alisa = #{sysAlisa}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="expire != null "> and expire = #{expire}</if>
        </where>
    </select>
    
    <select id="selectLogicFileById" parameterType="Long" resultMap="LogicFileResult">
        <include refid="selectLogicFileVo"/>
        where id = #{id}
    </select>
    
    <select id="selectLogicFileByCode" parameterType="java.lang.String" resultMap="LogicFileResult">
        <include refid="selectLogicFileVo"/>
        where code = #{code}  and status != 3
    </select>
        
    <insert id="insertLogicFile" parameterType="com.pewee.bean.LogicFile" useGeneratedKeys="true" keyProperty="id">
        insert into tbl_logic_file
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="code != null and code != ''">code,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="fileName != null and fileName != ''">file_name,</if>
            <if test="mimeType != null and mimeType != ''">mime_type,</if>
            <if test="length != null">length,</if>
            <if test="split != null">split,</if>
            <if test="total != null">total,</if>
            <if test="crc32 != null and crc32 != ''">crc32,</if>
            <if test="sysCode != null and sysCode != ''">sys_code,</if>
            <if test="sysAlisa != null and sysAlisa != ''">sys_alisa,</if>
            <if test="status != null">status,</if>
            <if test="expire != null">expire,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="code != null and code != ''">#{code},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="fileName != null and fileName != ''">#{fileName},</if>
            <if test="mimeType != null and mimeType != ''">#{mimeType},</if>
