            <if test="split != null">#{split},</if>
            <if test="total != null">#{total},</if>
            <if test="crc32 != null and crc32 != ''">#{crc32},</if>
            <if test="sysCode != null and sysCode != ''">#{sysCode},</if>
            <if test="sysAlisa != null and sysAlisa != ''">#{sysAlisa},</if>
            <if test="status != null">#{status},</if>
            <if test="expire != null">#{expire},</if>
         </trim>
    </insert>
    
